        "  print (NUMBER2)\n",
        "if NUMBER2 < NUMBER1 and NUMBER1 < NUMBER3:\n",
        "  print (NUMBER1)\n",
        "if NUMBER1 < NUMBER3 and NUMBER3 < NUMBER2:\n",
        "  print (NUMBER3)\n",
