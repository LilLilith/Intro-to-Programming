        "NUMBER = 6\n",
        "print(\"I am thinking of a number between 1 and 30\")\n",
        "GUESS = 0\n",
        "\n",
        "while GUESS != NUMBER:\n",
        "  print(\"Try to guess my number\")\n",
        "  GUESS = int(input())\n",
        "\n",
        "  if GUESS < NUMBER:\n",
        "    print('Your guess is too low.')\n",
        "  elif GUESS > NUMBER:\n",
        "    print('Your guess is too high.')\n",
        "  else:\n",
        "    break \n",
        "\n",
        "if GUESS == NUMBER:\n",
        "  print(\"You guessed my number\")"
