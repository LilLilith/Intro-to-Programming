        "def is_it_even(number):\n",
        "  if(number % 2 == 0):\n",
        "    return True\n",
        "  else:\n",
        "    return False\n",
        "\n",
        "OUTPUT=0\n",
        "INPUT=[]\n",
        "for loop in range (5):\n",
        "  INPUT.append(int(input(\"Please write a number:\")))\n",
        "if is_it_even(INPUT[0]):\n",
        "  OUTPUT = OUTPUT + 1\n",
        "if is_it_even(INPUT[1]):\n",
        "  OUTPUT = OUTPUT + 1  \n",
        "if is_it_even(INPUT[2]):\n",
        "  OUTPUT = OUTPUT + 11  \n",
        "if is_it_even(INPUT[3]):\n",
        "  OUTPUT = OUTPUT + 1\n",
        "if is_it_even(INPUT[4]):\n",
        "  OUTPUT = OUTPUT + 1\n",
        "print(\"This many numbers were even:\")\n",
        "print(OUTPUT)"
      ],
